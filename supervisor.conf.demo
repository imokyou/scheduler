[program:wechat_server]
command=/home/ptserver/.virtualenvs/scheduler/bin/python /home/ptserver/www/scheduler/scheduler/wechat_server.py
user=ptserver
autostart=true
redirect_stderr=true
stdout_logfile=/var/log/supervisor/wechat_server.log


[program:wechat_notify]
command=/home/ptserver/.virtualenvs/scheduler/bin/python /home/ptserver/www/scheduler/scheduler/notifier.py
user=ptserver
autostart=true
redirect_stderr=true
stdout_logfile=/var/log/supervisor/wechat_notify.log


[program:proxy_api]
command = /home/ptserver/.virtualenvs/scheduler/bin/gunicorn api:app -b localhost:5000 -w 2
directory = /home/ptserver/www/scheduler/scheduler
user = ptserver
autostart=true
redirect_stderr=true
stdout_logfile = /var/log/supervisor/scheduler.log



/home/ptserver/.virtualenvs/scheduler/bin/python /home/ptserver/www/scheduler/scheduler/messenger.py
